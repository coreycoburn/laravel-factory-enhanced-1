{
    "name": "makeabledk/laravel-factory-enhanced",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelFactory\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/database": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "doctrine/dbal": "^2.5",
        "laravel/laravel": "5.5.*|^6.0",
        "mockery/mockery": "^1.1",
        "nunomaduro/collision": "^1.1",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelFactory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelFactory\\FactoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
